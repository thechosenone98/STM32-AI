{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build project",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "type": "shell",
            "command": "C:/GNU/GnuWin32/bin/make.exe",
            "args": [
                "GCC_PATH=C:/GNU/GNU_Arm_Embedded_Toolchain/10-2020-q4-major/bin",
                "-j12"
            ],
            "problemMatcher": {
                "pattern": {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            },
            "presentation": {
                "focus": true
            }
        },
        {
            "label": "Compile current file",
            "type": "shell",
            "command": "C:/GNU/GNU_Arm_Embedded_Toolchain/10-2020-q4-major/bin/arm-none-eabi-gcc.exe",
            "args": [
                "-c",
                "-DUSE_HAL_DRIVER",
                "-DSTM32F446xx",
                "-IX-CUBE-AI/App",
                "-IX-CUBE-AI",
                "-IX-CUBE-AI/Target",
                "-ICore/Inc",
                "-IMiddlewares/ST/AI/Inc",
                "-IDrivers/STM32F4xx_HAL_Driver/Inc",
                "-IDrivers/STM32F4xx_HAL_Driver/Inc/Legacy",
                "-IDrivers/CMSIS/Device/ST/STM32F4xx/Include",
                "-IDrivers/CMSIS/Include",
                "-mcpu=cortex-m4",
                "-mthumb",
                "-mfpu=fpv4-sp-d16",
                "-mfloat-abi=hard",
                "-DUSE_HAL_DRIVER",
                "-DSTM32F446xx",
                "-IX-CUBE-AI/App",
                "-IX-CUBE-AI",
                "-IX-CUBE-AI/Target",
                "-ICore/Inc",
                "-IMiddlewares/ST/AI/Inc",
                "-IDrivers/STM32F4xx_HAL_Driver/Inc",
                "-IDrivers/STM32F4xx_HAL_Driver/Inc/Legacy",
                "-IDrivers/CMSIS/Device/ST/STM32F4xx/Include",
                "-IDrivers/CMSIS/Include",
                "-Og",
                "-Wall",
                "-fdata-sections",
                "-ffunction-sections",
                "-g",
                "-gdwarf-2",
                "-MMD",
                "-MP",
                "-MF'build/${fileBasenameNoExtension}.d'",
                "'${relativeFile}'",
                "-o",
                "'build/${fileBasenameNoExtension}.o'"
            ],
            "problemMatcher": {
                "pattern": {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            },
            "presentation": {
                "focus": true
            }
        },
        {
            "label": "Delete build folder",
            "type": "shell",
            "command": "C:/GNU/GnuWin32/bin/make.exe",
            "args": [
                "clean"
            ],
            "problemMatcher": [],
            "presentation": {
                "focus": false
            }
        },
        {
            "label": "CPU: Build, Download and run",
            "type": "shell",
            "command": "C:/GNU/GNUStuff/OpenOCD/0.10.0-12-20190422-2015/bin/openocd.exe",
            "args": [
                "-f",
                "C:/GNU/GNUStuff/OpenOCD/0.10.0-12-20190422-2015/scripts/interface/stlink.cfg",
                "-f",
                ".vscode/stm32f4x.cfg",
                "-c",
                "program build/AnomalyDetector.elf verify reset exit"
            ],
            "problemMatcher": [],
            "dependsOn": "Build project"
        },
        {
            "label": "CPU: Download and run",
            "type": "shell",
            "command": "C:/GNU/GNUStuff/OpenOCD/0.10.0-12-20190422-2015/bin/openocd.exe",
            "args": [
                "-f",
                "C:/GNU/GNUStuff/OpenOCD/0.10.0-12-20190422-2015/scripts/interface/stlink.cfg",
                "-f",
                ".vscode/stm32f4x.cfg",
                "-c",
                "program build/AnomalyDetector.elf verify reset exit"
            ],
            "problemMatcher": []
        },
        {
            "label": "CPU: Reset and run",
            "type": "shell",
            "command": "C:/GNU/GNUStuff/OpenOCD/0.10.0-12-20190422-2015/bin/openocd.exe",
            "args": [
                "-f",
                "C:/GNU/GNUStuff/OpenOCD/0.10.0-12-20190422-2015/scripts/interface/stlink.cfg",
                "-f",
                ".vscode/stm32f4x.cfg",
                "-c init",
                "-c reset",
                "-c exit"
            ],
            "problemMatcher": []
        },
        {
            "label": "CPU: Halt",
            "type": "shell",
            "command": "C:/GNU/GNUStuff/OpenOCD/0.10.0-12-20190422-2015/bin/openocd.exe",
            "args": [
                "-f",
                "C:/GNU/GNUStuff/OpenOCD/0.10.0-12-20190422-2015/scripts/interface/stlink.cfg",
                "-f",
                ".vscode/stm32f4x.cfg",
                "-c init",
                "-c halt",
                "-c exit"
            ],
            "problemMatcher": []
        },
        {
            "label": "CPU: Run",
            "type": "shell",
            "command": "C:/GNU/GNUStuff/OpenOCD/0.10.0-12-20190422-2015/bin/openocd.exe",
            "args": [
                "-f",
                "C:/GNU/GNUStuff/OpenOCD/0.10.0-12-20190422-2015/scripts/interface/stlink.cfg",
                "-f",
                ".vscode/stm32f4x.cfg",
                "-c init",
                "-c resume",
                "-c exit"
            ],
            "problemMatcher": []
        },
        {
            "label": "Run Python file",
            "type": "shell",
            "command": "python",
            "args": [
                "${file}"
            ],
            "presentation": {
                "focus": true
            },
            "problemMatcher": []
        },
        {
            "label": "Open CubeMX project",
            "type": "shell",
            "command": "start",
            "args": [
                "AnomalyDetector.ioc"
            ],
            "presentation": {
                "focus": false
            },
            "problemMatcher": []
        },
        {
            "label": "Update workspace",
            "type": "shell",
            "command": "python",
            "args": [
                "${workspaceFolder}/ideScripts/update.py"
            ],
            "presentation": {
                "focus": true
            },
            "problemMatcher": []
        }
    ]
}